{
	"name": "axllent/silverstripe-links-to-blank",
	"description": "Extension to add JavaScript to all pages to add target=\"_blank\" to all outgoing links & downloads",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/axllent/silverstripe-links-to-blank",
	"keywords": ["silverstripe", "javascript", "links"],
	"license": "The MIT License (MIT)",
	"authors": [
		{
			"name": "Ralph Slooten",
			"homepage": "http://www.axllent.org/"
		}
	],
	"support": {
		"issues": "https://github.com/axllent/silverstripe-links-to-blank/issues"
	},
    "require": {
        "silverstripe/vendor-plugin": "^1.0",
		"silverstripe/cms": "^4.0"
	},
    "extra": {
        "expose": [
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "Axllent\\LinksToBlank\\": "src/"
        }
    }
}
